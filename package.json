{
  "name": "pothos-codegen",
  "version": "0.1.0",
  "license": "MIT",
  "description": "A prisma generator that generate input types for Pothos. And optionally generate all crud. Fork of https://github.com/Cauen/prisma-generator-pothos-codegen",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./src/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.js"
  },
  "scripts": {
    "test": "vitest",
    "swc_compile": "swc src --out-dir=dist",
    "build": "npm run swc_compile"
  },
  "bin": {
    "pothos-codegen": "./dist/src/bin.js"
  },
  "peerDependencies": {
    "@pothos/core": "^4.0.2",
    "@pothos/plugin-prisma": "^4.0.3",
    "@prisma/client": "^5.0.0",
    "prisma": "^5.0.0"
  },
  "devDependencies": {
    "@prisma/internals": "6.8.2",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.11.24",
    "@types/node": "^22.15.21",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.4"
  },
  "dependencies": {
    "@prisma/generator-helper": "6.8.2"
  }
}